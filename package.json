{
  "name": "svelte-starter-kit",
  "private": true,
  "version": "0.0.0",
  "description": "PNPM + Turborepo + Sveltekit",
  "author": "Sumanth Chinthagunta",
  "keywords": [
    "Sveltekit",
    "Svelte",
    "PNPM",
    "Turborepo",
    "Monorepo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xmlking/svelte-starter-kit.git"
  },
  "bugs": {
    "url": "https://github.com/xmlking/svelte-starter-kit/issues"
  },
  "license": "MIT",
  "workspaces": [
    "apps/**",
    "packages/**"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "clean": "git clean -fXd",
    "untracked": "git clean -f -d"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.1",
    "eslint": "8.42.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "2.8.8",
    "prettier-plugin-svelte": "2.10.1",
    "turbo": "1.10.7"
  },
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "pnpm": ">=8.6.0"
  },
  "type": "module"
}
